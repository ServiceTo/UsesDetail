{
    "name": "service-to/uses-detail",
    "description": "A Laravel trait for handling dynamic model attributes using a JSON detail column",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "ServiceTo",
            "email": "info@service.to"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/database": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ServiceTo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ServiceTo\\UsesDetail\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
} 